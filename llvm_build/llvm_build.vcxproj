  <PropertyGroup Label="UserMacros">
    <CompilerArgs>/D_SILENCE_CXX17_ITERATOR_BASE_CLASS_DEPRECATION_WARNING=1 /D_HAS_EXCEPTIONS=0 -fdiagnostics-absolute-paths /clang:-m64</CompilerArgs>
    <CmakeCLI>set CXXFLAGS="$(CompilerArgs)"
	set CFLAGS="$(CompilerArgs)"
	cmake -G "Ninja Multi-Config" -DCMAKE_CONFIGURATION_TYPES:STRING="Debug;Release" -DCMAKE_C_COMPILER:FILEPATH="clang-cl.exe" -DCMAKE_CXX_COMPILER:FILEPATH="clang-cl.exe" -DLLVM_TARGETS_TO_BUILD:STRING="X86" -DLLVM_ENABLE_BINDINGS:BOOL=OFF -DLLVM_INCLUDE_RUNTIMES:BOOL=OFF -DLLVM_ENABLE_IDE:BOOL=OFF -DLLVM_INCLUDE_DOCS:BOOL=OFF -DLLVM_INCLUDE_EXAMPLES:BOOL=OFF -DLLVM_INCLUDE_TESTS:BOOL=OFF -DLLVM_INCLUDE_TOOLS:BOOL=OFF -DLLVM_INCLUDE_UTILS:BOOL=OFF  -DLLVM_INCLUDE_BENCHMARKS:BOOL=OFF -DLLVM_ENABLE_DUMP:BOOL=ON -DLLVM_OPTIMIZED_TABLEGEN:BOOL=ON -DCMAKE_CXX_STANDARD:STRING="17" ../llvm-project/llvm</CmakeCLI>
    <CmakeBuild>cmake --build . --config "$(Configuration)"</CmakeBuild>
    <CleanCmakeCacheCMD>for /F "delims= eol=|" %%f in ('
  dir /b ^| findstr /V "llvm_build[^.]*\.vcxproj"
') do (
  echo deleting .\%%f
  if exist %%f\nul (
    rmdir .\%%f /s /q
  ) else (
    del .\%%f /q
  )
)</CleanCmakeCacheCMD>
